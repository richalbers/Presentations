If statements
	The following examples shows the object code generated from this:
	[[code]]
		int funcIfs() {                             
			int x=2;
			int y=3;
				
			// if
			if (x==4) {
				x=x+5;
				y=y+2;
			}
				
			// if else
			if (y==7) {
				x--;
				y--;
			} else {
				x++;
				y++;
			}
				
			return y;
		}
